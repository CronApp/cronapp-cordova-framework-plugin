  <name>CronAppCordovaFrameworkPlugin</name>
  <description>You can use the CronAppPlugin plugin to update CronApp Mobile App.</description>
  <license>MIT</license>
  <repo>https://github.com/cronapp/cronapp-cordova-framework-plugin.git</repo>
  <issue>https://github.com/cronapp/cronapp-cordova-framework-plugin/issues</issue>
  <engines>
    <engine name="cordova" version=">=7.1.0"/>
    <engine name="cordova-android" version=">=6.3.0"/>
  </engines>
